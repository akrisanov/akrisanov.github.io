{
    "name": "Witch Hazel Light",
    "type": "light",
    "colors": {
        "editor.background": "#fafaf8",
        "editor.foreground": "#1f2328",
        "editor.selectionBackground": "#d8e8de",
        "editor.lineHighlightBackground": "#f1f4ee",
        "editorCursor.foreground": "#1f6a4a",
        "editorWhitespace.foreground": "#a8b2ab"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#6a737d",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#0f6f5c"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#9a4d00"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#8a3f21"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant.character, constant.other",
            "settings": {
                "foreground": "#8a3f21"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#0550ae"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#7a2e59"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#7a2e59"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7a2e59"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.class",
            "settings": {
                "fontStyle": "",
                "foreground": "#0d5f89"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#0d5f89"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#1f6a4a"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#0550ae"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#0d5f89"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#1f6a4a"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#1f6a4a"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#8a3f21"
            }
        },
        {
            "name": "Library class/type",
            "scope": "support.type, support.class",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#0d5f89"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "",
                "foreground": "#ffffff",
                "background": "#c62828"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#ffffff",
                "background": "#7a2e59"
            }
        },
        {
            "name": "Markdown header",
            "scope": "markup.heading.markdown",
            "settings": {
                "foreground": "#1f6a4a",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown inline link",
            "scope": "meta.link.inline.markdown",
            "settings": {
                "foreground": "#2d6a4f"
            }
        },
        {
            "name": "Markdown reference link",
            "scope": "meta.link.reference.markdown",
            "settings": {
                "foreground": "#2d6a4f"
            }
        },
        {
            "name": "Markdown inline link title",
            "scope": "string.other.link.title.markdown",
            "settings": {
                "foreground": "#0d5f89",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown inline link url",
            "scope": "markup.underline.link.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown inline link url",
            "scope": "markup.underline.link.image.markdown",
            "settings": {
                "fontStyle": "underline italic"
            }
        },
        {
            "name": "Markdown bold",
            "scope": "markup.bold.markdown",
            "settings": {
                "foreground": "#1f2328",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": "markup.italic.markdown",
            "settings": {
                "foreground": "#1f2328",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown image",
            "scope": "meta.image.inline.markdown",
            "settings": {
                "foreground": "#7a2e59",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown image title",
            "scope": "string.other.link.description.markdown",
            "settings": {
                "foreground": "#0d5f89",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Markdown inline code",
            "scope": "markup.inline.raw.string.markdown",
            "settings": {
                "foreground": "#8a3f21"
            }
        },
        {
            "name": "Markdown fenced code",
            "scope": "markup.fenced_code.block.markdown",
            "settings": {
                "foreground": "#1f2328"
            }
        },
        {
            "name": "Markdown fenced code language name",
            "scope": "fenced_code.block.language.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown fenced code content",
            "scope": "meta.embedded.block",
            "settings": {
                "foreground": "#1f2328"
            }
        },
        {
            "name": "Markdown list",
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#5a636d"
            }
        }
    ]
}
